.schema albums
SELECT
  concat (first_name, ' ', last_name) AS "full_name"
FROM
  "user"
  concat (AlbumId, ' ', Title) AS "full_name"
  "albums"
SELECT 'SQLite ' || 'CONCAT';
    FirstName || ' ' || LastName AS FullName
    Employees
ORDER BY
    FullName;
